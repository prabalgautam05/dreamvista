{
  "version": 2,
  "builds": [
    {
      "src": "dreamvista_backend/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
    { "src": "/(.*)", "dest": "dreamvista_backend/wsgi.py" }
  ]
}